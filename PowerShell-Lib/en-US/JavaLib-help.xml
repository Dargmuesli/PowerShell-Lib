    <command:details>
      <command:name>Test-JDKInstalled</command:name>
      <command:verb>Test</command:verb>
      <command:noun>JDKInstalled</command:noun>
      <maml:description>
        <maml:para>Checks whether a JDK is installed.</maml:para>
      </maml:description>
    </command:details>
    <maml:description>
      <maml:para>The "Test-JDKInstalled" cmdlet tests if an app with a name matching a regular expression for valid JDK names is installed and returns true if that's the case.</maml:para>
    </maml:description>
    <command:syntax>
      <command:syntaxItem>
        <maml:name>Test-JDKInstalled</maml:name>
      </command:syntaxItem>
    </command:syntax>
    <command:parameters />
    <command:inputTypes />
    <command:returnValues />
    <maml:alertSet>
      <maml:alert>
        <maml:para>Matches:     Java(TM) SE Development Kit 8     Java(TM) SE Development Kit 8 (64-bit)     Java(TM) SE Development Kit 8 Update 152     Java(TM) SE Development Kit 8 Update 152 (64-bit)     Java(TM) SE Development Kit 9     Java(TM) SE Development Kit 9 (64-bit)     Java(TM) SE Development Kit 9.0.1     Java(TM) SE Development Kit 9.0.1 (64-bit)</maml:para>
      </maml:alert>
    </maml:alertSet>
